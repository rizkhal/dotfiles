[alias]
    all = add .
    s = status
    b = branch
    cm = commit -m
    cms = commit -S -m
    co = checkout
    cp = checkout -b
    d = branch -d
    fd = branch -D
    lg = !git log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
    rb = rebase
    prb = pull --rebase
    rv = remote -v
